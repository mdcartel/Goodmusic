{
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_ENV": "production"
    }
  },
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD"
}