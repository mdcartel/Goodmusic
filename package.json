{
  "name": "goodmusic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "build:analyze": "ANALYZE=true npm run build",
    "build:standalone": "BUILD_STANDALONE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest --testPathIgnorePatterns=e2e/",
    "test:watch": "jest --watch --testPathIgnorePatterns=e2e/",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=e2e/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run type-check && npm run lint && npm run test && npm run test:e2e",
    "clean": "rimraf .next node_modules/.cache",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install || true",
    "precommit": "npm run lint:fix && npm run type-check",
    "prebuild": "echo 'Skipping prebuild checks for Vercel deployment'",
    "verify-build": "node scripts/verify-build.js",
    "postbuild": "echo 'Build completed successfully'",
    "setup": "node scripts/setup-ytdlp.js",
    "setup:audio": "node scripts/setup-audio-extraction.js",
    "test:integration": "node scripts/test-integration.js",
    "test:audio-extraction": "jest src/lib/services/__tests__/audio-extraction.test.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slider": "^1.3.5",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.535.0",
    "next": "15.4.5",
    "node-id3": "^0.2.9",
    "react": "19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "youtube-dl-exec": "^3.0.22",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.5",
    "@playwright/test": "^1.54.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
